function change_category_select(category_code) {

  $('#proposal_exam_id').select2("val", "");
  $("#proposal_exam_id").select2("readonly", true);

  if (category_code !== "") {
    if (category_code !== $('#proposal_division_id').data('category')) {
      $('#proposal_division_id').data('category', category_code); 
      $('#proposal_division_id').select2("val", "");
      $("#proposal_division_id").select2("readonly", false);

      $('#proposal_exam_id').data('category', category_code); 
    };
  } else {
    $('#proposal_division_id').data('category', ""); 
    $('#proposal_division_id').select2("val", "");
    $("#proposal_division_id").select2("readonly", true);

    $('#proposal_exam_id').data('category', ""); 
  };


};