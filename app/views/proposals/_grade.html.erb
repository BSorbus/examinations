<% testportal_test_info_name = grade["testportal_test_info"] %>
<% access_code = '"' + grade["testportal_access_code_id"] + '"' %>
<% to_value = '{  "autoSubmit": true, 
									"startPageReadOnly": true, 
									"personUID": '+"#{@proposal.multi_app_identifier}"+', 
									"accessCode": ' + "#{access_code}" + ', 
									"personalData": { 
										"firstName": '+"#{@current_user.first_name}"+', 
										"lastName": '+"#{@current_user.last_name}"+' } }' %>

<div class="row">
	<p><%= testportal_test_info_name %></p>
	<p><%= access_code %></p>
</div>


<div class="row">
	<div class="col-sm-12">
		<form target="test-frame" method="post" action="https://egzaminyuke.testportal.pl/exam/start.html">
			<input name="startTestRequest" type="hidden" value="<%= to_value %>">
			<input type="submit" class="btn btn-danger btn-grader" value="<%= testportal_test_info_name %>" onclick="testportal_submit_click(this)" >
		</form>
		<br>
	</div>
</div>
